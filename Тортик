#include <iostream>
using namespace std;
const float pi = 3.14;

int main()
{
    float Diam1, Diam2, S1, S2, Final;
    
    cout << "Введите диаметр будущего тортика: ";
    cin >> Diam1;
    cout << "Введите диаметр вашей формы: ";
    cin >> Diam2;
    
    S1 = Diam1 / 2 * Diam1 / 2;
    S2 = Diam2 / 2 * Diam2 / 2;
    
    Final = S1 / S2;
    
    cout << "Коэффицент: " << Final;
    
}
